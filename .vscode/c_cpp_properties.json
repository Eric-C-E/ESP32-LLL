{
  "configurations": [
    {
      "name": "ESP-IDF",
      "compilerPath": "${config:idf.toolsPath}/tools/xtensa-esp-elf/esp-15.2.0_20251204/xtensa-esp-elf/bin/xtensa-esp32s3-elf-gcc",
      "intelliSenseMode": "linux-gcc-x64",
      "compileCommands": "${workspaceFolder}/ESP32-LLL/build/compile_commands.json",
      "includePath": [
        "${config:idf.espIdfPath}/components/**",
        "${config:idf.espIdfPathWin}/components/**",
        "${workspaceFolder}/ESP32-LLL/**",
        "${workspaceFolder}/ESP32-LLL/build/esp-idf/**",
        "${workspaceFolder}/ESP32-LLL/managed_components/**",
        "${env:IDF_PATH}/components/**"
      ],
      "browse": {
        "path": [
          "${config:idf.espIdfPath}/components",
          "${config:idf.espIdfPathWin}/components",
          "${workspaceFolder}/ESP32-LLL"
        ],
        "limitSymbolsToIncludedHeaders": true
      }
    }
  ],
  "version": 4
}
